  },
  "homepage": "https://github.com/DmitryKorolDevPro/MentorshipProgramRemoteProject#readme",
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/node": "14.14.41",
    "live-server": "1.2.1",
    "nodemon": "2.0.7",
    "typescript": "4.2.4"
  }
