{
  "name": "e-shop",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run backend-start && npm run frontend-start",
    "backend-start": "node backend/prod/app.js",
    "backend-dev": "node node_modules/nodemon/bin/nodemon.js -e ts --exec npm run backend-start",
    "frontend-start": "cd frontend && live-server",
    "frontend-dev": "nodemon --ext ts --exec tsc -p frontend/tsconfig.json && node backend/prod/app.js"
  },
